--(TOP_Revive_Script)--
--(By: helpguestslikeme)--
--(Version 2.1)--

local plr = game:GetService("Players").LocalPlayer
game.Workspace:WaitForChild("Live")
game.Workspace.Live:WaitForChild(plr.Name)
local Char = plr.Character
local portal = game.workspace["Wormhole"]
local Mouse = plr:GetMouse()

game:GetService("StarterGui"):SetCore("SendNotification", {
	Title = "Press L then K";
	Text = "Created by: Isabella";
})

local lplr = game:GetService("Players").LocalPlayer
local mouse = lplr:GetMouse()

mouse.KeyDown:connect(function(key)
    if key == "k" then
        game:GetService("TweenService"):Create(lplr.Character.HumanoidRootPart, TweenInfo.new(0.3, Enum.EasingStyle.Linear), {CFrame = CFrame.new(100, 100, 100)}):Play()
    end
    end)

mouse.KeyDown:connect(function(key)
    if key == "l" then
        local action = lplr.Character.SuperAction
        action:Destroy()
    end
    end)
